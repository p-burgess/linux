;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.obss.local. root.obss.local. (
		      2012022202	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.obss.local.
@	IN	A	127.0.0.1
@	IN	AAAA	::1
ns	IN	A	192.168.254.161
external	IN 	A	192.168.254.161
www	IN 	A	192.168.254.161
